// In the name of Alah, the most merciful, the most gracious
#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define mh ios::sync_with_stdio(false); cin.tie(0);
#define nl '\n'

const int N = 1e5 + 5;

int a[N], dp[N];

int main()
{
    mh
    ll n, k;
    cin >> n >> k;
    for (int i = 1; i <= n; i++) cin >> a[i];

    // dp[x] = 1 if current player can win with x stones
    dp[0] = 0; // 0 stones â†’ lose

    for (int x = 1; x <= k; x++)
    {
        dp[x] = 0; // Assume losing
        for (int i = 1; i <= n; i++)
        {
            if (a[i] <= x && dp[x - a[i]] == 0)
            {
                dp[x] = 1; // If any move leads to opponent losing, I win
                break;
            }
        }
    }

    cout << (dp[k] ? "First" : "Second") << nl;
    return 0;
}
